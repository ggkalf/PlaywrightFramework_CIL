{
  "name": "playwrightframework",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clearResults": "cross-env testEnv=int03 npx playwright test src/specs/clear_results.spec.js --project=chromium",
    "dryrun": "cross-env testEnv=load01 npx playwright test src/specs/LOAD01_DRY_RUN/Load01-dry-run.spec.js",
    "howtoplayTest:Chromium": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-fpg-navigate-to-how-to-play-page.spec.js --project=chromium --repeat-each=20",
    "howtoplayTest:firefox": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-fpg-navigate-to-how-to-play-page.spec.js --project=firefox --repeat-each=2",
    "howtoplayTest:safari": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-fpg-navigate-to-how-to-play-page.spec.js --project=safari --repeat-each=2",
    "howtoplayTest:mobile-safari": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-fpg-navigate-to-how-to-play-page.spec.js --project='mobile safari' --repeat-each=2",
    "purchaseTickets": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-purchase-fpg-tickets.spec.js --project='chromium' --repeat-each=1",
    "fiesta": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-fiesta-fever-iframe.spec.js --repeat-each=1",
    "pbb": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-piggy-bank-bucks-iframe.spec.js --repeat-each=1",
    "bnk": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-big-number-knockout-iframe.spec.js --repeat-each=1",
    "bbh": "cross-env testEnv=int03 npx playwright test src/specs/REL42/REL42-big-bucks-highway-iframe.spec.js --repeat-each=1"
  },
