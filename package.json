{
  "name": "playwrightframework",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clearResults": "cross-env testEnv=int03 npx playwright test src/specs/Cleanup/clear_results.spec.js --project=chromium",
    "dryrun": "cross-env testEnv=load01 npx playwright test src/specs/Load01DryRun/Load01-dry-run.spec.js",
    "howtoplay": "cross-env testEnv=int03 npx playwright test src/specs/REL42/FpgNavSpec/REL42-fpg-navigate-to-how-to-play-page.spec.js --repeat-each=20",
    "fiesta": "cross-env testEnv=int03 npx playwright test src/specs/REL42/FpgIframeSpec/REL42-fiesta-fever-iframe.spec.js --repeat-each=20",
    "pbb": "cross-env testEnv=int03 npx playwright test src/specs/REL42/FpgIframeSpec/REL42-piggy-bank-bucks-iframe.spec.js --project=chromium --repeat-each=1",
    "bnk": "cross-env testEnv=int03 npx playwright test src/specs/REL42/FpgIframeSpec/REL42-big-number-knockout-iframe.spec.js --repeat-each=20",
    "iframeSpec": "cross-env testEnv=int03 npx playwright test src/specs/REL42/FpgIframeSpec --repeat-each=20",
    "clearIframeCSV": "cross-env testEnv=int03 npx playwright test src/specs/Cleanup/clearIframeCSV.spec.js --project=chromium",
    "helpers": "ts-node src/utils/helpers.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.30.0",
    "cross-env": "^7.0.3",
    "playwright": "^1.30.0"
  },
  "dependencies": {
    "csv": "^6.2.0",
    "csv-stringify": "^6.2.4",
    "dotenv": "^16.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ggkalf/PlaywrightFramework_CIL.git"
  },
